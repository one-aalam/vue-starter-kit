{
  "name": "vue-starter-kit",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch ../../dist/index.js -x \"cross-env vite\"",
    "dev:ssr": "nodemon --watch ../../dist/index.js -x \"cross-env vite-ssr\"",
    "build": "vuedx-typecheck . && cross-env vite build",
    "dev:d": "nodemon --watch ../../dist/index.js -x \"cross-env DEBUG=vite-plugin-layouts vite\"",
    "clean": "rm -rf dist && rm -rf serverless/public && rm -rf serverless/api/renderer",
    "build:d": "npm run clean && cross-env DEBUG=vite-plugin-layouts NODE_ENV=production vite-ssr build && ./scripts/move-dist.sh",
    "run-build": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true vite build",
    "run-build-claims": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true CLAIMS=true vite build",
    "start": "npm run run-build && npm run serve:https",
    "start-claims": "npm run run-build-claims && npm run serve:https",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "serve": "vite preview",
    "serve:https": "serve dist",
    "preview": "cd serverless && vercel dev",
    "deploy": "cd serverless && vercel",
    "deploy:prod": "cd serverless && vercel --prod"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.11.7",
    "@vueuse/core": "^4.9.0",
    "@vueuse/head": "^0.5.1",
    "nprogress": "^0.2.0",
    "redaxios": "^0.4.1",
    "vite-ssr": "^0.8.0",
    "vue": "3.0.9",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.322",
    "@intlify/vite-plugin-vue-i18n": "^2.0.4",
    "@rollup/plugin-replace": "^2.4.2",
    "@tailwindcss/jit": "^0.1.17",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@types/node": "^14.14.37",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-vue": "^1.2.0",
    "@vue/compiler-sfc": "^3.0.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/server-renderer": "^3.0.11",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "vite": "^2.1.3",
    "vite-plugin-components": "^0.8.3",
    "vite-plugin-eslint": "^1.1.0",
    "vite-plugin-fonts": "^0.2.0",
    "vite-plugin-icons": "^0.4.0",
    "vite-plugin-pages": "^0.9.0",
    "vite-plugin-pwa": "^0.6.4",
    "vite-plugin-restart": "^0.0.1",
    "vite-plugin-vue-layouts": "^0.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  }
}
